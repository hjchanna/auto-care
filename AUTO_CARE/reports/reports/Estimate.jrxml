	<queryString>
		<![CDATA[SELECT 
	t_job_item.item_type as item_type,
	t_job_item.quantity as quantity,
	t_job_card.number as job_number,
	t_job_card.in_time as in_time,
	m_vehicle.vehicle_no as vehicle_no,
	m_item.name as item	
FROM t_job_item
	LEFT JOIN t_job_card on t_job_card.index_no = t_job_item.job_card
	LEFT JOIN m_vehicle on m_vehicle.index_no = t_job_card.vehicle
	LEFT JOIN m_item on m_item.index_no = t_job_item.item 
WHERE
	t_job_card.index_no=  $P{INDEX_NO} and
	t_job_item.item IS NOT NULL]]>
	</queryString>
